{
    "name": "ritaorion/laravel-actions-enums",
    "description": "This package provides Artisan CLI Support to create Scaffolding for Actions and Enum classes in Laravel",
    "keywords": [
        "laravel",
        "laravel-actions-enums"
    ],
    "license": "MIT",
    "homepage": "https://github.com/ritaorion/laravel-actions-enums",
    "require": {
        "php": ">=8.1",
        "spatie/laravel-package-tools": "^1.14.0",
        "spatie/php-structure-discoverer": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Riritaorion\\LaravelActionsEnums\\": "app"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi"
    },
    "authors": [
        {
            "name": "Andrew Godby",
            "email": "andrewmgodby@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Riritaorion\\LaravelActionsEnums\\LaravelActionsEnumsServiceProvider"
            ],
            "aliases": {
                "LaravelActionsEnums": "Riritaorion\\LaravelActionsEnums\\Facades\\LaravelActionsEnums"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
